# Environment Variables for Quiz AI App

# Gemini API Key (Required)
# Copy this file to `.env` and set your real key. Do NOT commit your `.env`.
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE

# Optional session secret for signing session tokens (development only)
# SESSION_SECRET=replace_with_a_random_secret

# Node Environment (optional)
NODE_ENV=development
